<p id="notice"><%= notice %></p>

<h1>Api Keys</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sort</th>
      <th>Open</th>
      <th>Secret</th>
      <th>User</th>
      <th>Exchenge</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_keys.each do |api_key| %>
      <tr>
        <td><%= api_key.name %></td>
        <td><%= api_key.sort %></td>
        <td><%= api_key.open %></td>
        <td><%= api_key.secret %></td>
        <td><%= api_key.user %></td>
        <td><%= api_key.exchange %></td>
        <td><%= link_to 'Show', api_key %></td>
        <td><%= link_to 'Edit', edit_api_key_path(api_key) %></td>
        <td><%= link_to 'Destroy', api_key, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Key', new_api_key_path %>
